@{
    Layout = "_LayoutClinician";
}
    <div class="row">
        <form class="col-12 col-md-6 offset-md-3 col-xl-3 offset-xl-1">   
            <div class="form-group">
                <label for="clinicianEmail" class="text-muted">Email:</label>
                <input type="email" id="clinicianEmail" name="clinicianEmail" value="" placeholder="Clinician@Mail.com" class="form-control form-control-sm" />
            </div>
            <div class="form-group">
                <label for="clinicianPassword" class="text-muted">Password:</label>
                <input type="password" id="clinicianPassword" name="clinicianPassword" value="" placeholder="********" class="form-control form-control-sm" />
            </div>
            <button type="submit" class="btn btn-primary btn-sm btn-block">Login</button>

        </form>
    </div>